public class Yikes
{
	public static void go(Integer n)
	{
		System.out.println("Long");
	}
	public static void go(Short n)
	{
		System.out.println("Short");
	}
	public static void go(long n)
	{
		System.out.println("int");
	}
	public static void main(String[] args) {
		short y=6;
		Long z=new Long(7);
		go(y);
		go(y);
	}
}