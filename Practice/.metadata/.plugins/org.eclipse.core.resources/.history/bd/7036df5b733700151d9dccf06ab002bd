

class Item
{
	int x;
	Inner in=new Inner();
	Item()
	{
		x=10;
	}
	void disp()
	{
		System.out.println("Main class"+x);
		System.out.println(in.y);
	}
	class Inner
	{
		int y;
		Inner()
		{
			y=20;
		}
		void disp2()
		{
			Item it=new Item();
			System.out.println("Inner class"+y);
			System.out.println(x);
		}
	}
}

class ItemTest
{
	public static void main(String[] args) 
	{
		Item it=new Item();
		it.disp();
		System.out.println(it.in.y);
		it.in.disp2();
	}
}