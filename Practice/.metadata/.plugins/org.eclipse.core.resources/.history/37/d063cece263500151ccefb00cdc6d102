import java.util.*;

class ThreadExmp extends Thread
{
	static int count=0;
	int a,b;
	ThreadExmp(int a,int b)
	{
		this.a=a;
		this.b=b;
	}
	public int add()
	{
		return a+b;
	}
	public int sub()
	{
		return a-b;
	}
	public void run()
	{
		if(count%2==0)
		{
			int res=add();
			count++;
			System.out.println(res);
		}
		else
		{
			count++;
			int res=sub();
			System.out.println(res);
		}
	}
}

class DiffMethodsThreadWithoutName 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter 2 Numbers");
		int a=scan.nextInt();
		int b=scan.nextInt();
		ThreadExmp e1=new ThreadExmp(a,b);
		ThreadExmp e2=new ThreadExmp(a,b);
		e1.start();
		e2.start();
	}
}