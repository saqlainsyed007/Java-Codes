class Demo1
{
	static int x=10;
	public void m1()
	{
		System.out.println("M1");
	}
}
class Demo2 extends Demo1
{
	Demo2()
	{
		x=4;
	}
	public void m1()
	{
		System.out.println("M1 version 2");
	}
}
class ItemTest
{
	public static void main(String[] args)
	{
		Demo1 d= new Demo2();
		System.out.println(d.x);
		d.m1();
	}
}